    "url": "git+https://github.com/alemagio/fastify-socket.io.git"
  },
  "license": "MIT",
  "dependencies": {
    "fastify-plugin": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "fastify": "^3.24.0",
    "fastify-tsconfig": "^1.0.1",
    "standard": "^16.0.4",
    "tap": "^15.1.5",
    "ts-standard": "^10.0.0",
    "tsd": "^0.19.0",
    "typescript": "^4.5.2"
  },
  "keywords": [
