{
  "name": "fastify-socket.io",
  "version": "5.1.0",
  "private": false,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --noEmit && tsup-node",
    "format": "rome format --write .",
    "lint": "rome check .",
    "prepare": "husky install",
    "prepublish": "npm build",
    "release": "pnpm build && changeset publish",
    "test": "tsx --test ./test/*.ts",
    "test:watch": "tsx --watch --test ./test/*.ts",
    "test:coverage": "c8 --all --src src --reporter lcov --reporter text tsx --test ./test/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ducktors/fastify-socket.io.git"
  },
  "dependencies": {
    "fastify-plugin": "^5.0.1",
    "tslib": "^2.6.1"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@ducktors/tsconfig": "^1.0.0",
    "@types/node": "^20.14.10",
    "c8": "^8.0.1",
    "fastify": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "rome": "12.1.3",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "tsup": "^8.3.4",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "fastify": ">=4",
    "socket.io": ">=4"
  }
}
